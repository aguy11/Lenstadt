& Random Fibbonacci Sequence! &

use random;

defFunc fib ( amount ) {
  fib1 = 0;
  fib2 = 1;
  loop i from 1 to amount {
    fibsaver = fib1;
    fib1 = fib2;
    fib2 = fib2 + fibsaver;
  } 
  give fib2;
}

completeWhile True {
  looping_time = captureInt "How many times to find fibbonacci number?";
  completeIf looping_time <= 2 {
    stampLn "Must Choose 3 or more!";
  }
  completeElseIf looping_time >= 1000 {
    stampLn "The number cannot be above 1000. Try again.";
  }
  completeElse {
    quitLoop;
  }
}
num = 1;
completeWhile num <= looping_time {
  amount_to_do = randomint(5,20);
  finished_fib = fib(amount_to_do);
  stampLn "{finished_fib} is number {amount_to_do} in the fibbonacci sequence.";
  num += 1;
}

